<h1 class="text-center mb-3">Member</h1>
<form action="/api/members" method="POST" class="list-group">
    <div class="form-group">
        <lable for="name" >Name</lable>
        <input type="text" name="name" class="form-control">
    </div>
    <div class="form-group">
        <lable for="email">Email</lable>
        <input type="text" name="email" class="form-control">
    </div>
    <input type="submit" value="add member" class="btn btn-primary btn-block">

</form>

<h4>Members</h4>
<ul>
    {{#each members}}
        <li class="list-group-item">{{this.name}} : {{this.email}}</li>
    {{/each}}
</ul>